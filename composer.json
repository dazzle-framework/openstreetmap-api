{
    "name": "dazzlesoftware/openstreetmap",
    "type": "library",
    "description": "Dazzle OpenStreetMap Package",
    "keywords": ["dazzle", "framework", "openstreetmap"],
    "homepage": "https://github.com/dazzle-framework/openstreetmap-api",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0",
        "dazzlesoftware/application": "^1.0|^2.0",
        "dazzlesoftware/http": "^1.0|^2.0",
        "dazzlesoftware/input": "^1.0|^2.0",
        "dazzlesoftware/oauth1": "^1.0|^2.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0|^2.0",
        "dazzlesoftware/test": "^1.0|^2.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\OpenStreetMap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\OpenStreetMap\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-develop": "2.x-dev"
        }
    }
}
